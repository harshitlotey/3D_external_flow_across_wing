FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 2.34375;

boundaryField
{
    inlet1
    {
        type            fixedValue;
        // inletValue      uniform 2.34375;
        value           uniform 2.34375;
    }
    inlet2
    {
        type            fixedValue;
        // inletValue      uniform 2.34375;
        value           uniform 2.34375;
    }
    outlet1
    {
        type            inletOutlet;
        inletValue      uniform 2.34375;
        value           uniform 2.34375;
    }
    symmetryPlane
    {
        type            symmetry;
    }
    wall1
    {
        type            fixedValue;
        value           uniform 1e-10;
    }
    defaultBoundary
    {
        type            inletOutlet;
        inletValue      $internalField; // Auto-grabs value from top of file
        value           $internalField;
    }
}


