FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{

        // Valid symmetric matrix solver types :
		// 7(FPCG GAMG PBiCGStab PCG PPCG PPCR smoothSolver)

    p
    {
        solver          GAMG;
        //preconditioner  DIC;
        tolerance       1e-06;
        relTol          1e-02;
        smoother        GaussSeidel;
    }
    Phi
    {
        solver          GAMG;
        tolerance       1e-7;
        relTol          0.01;
        smoother        GaussSeidel;
    }
    "(U|k|epsilon|omega)"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-08;//
        relTol          1e-01;//
    }
}
SIMPLE
{
    nNonOrthogonalCorrectors 1;
    consistent      no;
    residualControl
    {
        p               1e-6;//
        U               1e-6;//
        "(k|epsilon|omega)" 1e-5;//
    }
}
relaxationFactors
{
    equations
    {
        U               0.7; //
        k               0.5; //
        epsilon         0.5;//
        omega           0.5;//
    }
	fields
	{
		p               0.3;//
	}
}


